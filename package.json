{
  "name": "codespaces-nextjs",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint . --ext .js,.jsx",
    "predb:seed": "npm run db:generate",
    "db:generate": "dotenv -e .env.local -- prisma generate --schema=./db/prisma/schema.prisma",
    "db:push": "dotenv -e .env.local -- prisma db push --schema=./db/prisma/schema.prisma",
    "db:migrate": "dotenv -e .env.local -- prisma migrate deploy --schema=./db/prisma/schema.prisma",
    "db:studio": "dotenv -e .env.local -- prisma studio --schema=./db/prisma/schema.prisma",
    "db:seed": "dotenv -e .env.local -- node ./db/seeds/index.js",
    "db:start": "scripts/shared/docker-db.sh start",
    "db:stop": "scripts/shared/docker-db.sh stop",
    "db:status": "scripts/shared/docker-db.sh status",
    "db:logs": "scripts/shared/docker-db.sh logs"
  },
  "dependencies": {
    "@prisma/client": "^5.19.0",
    "bcryptjs": "^3.0.3",
    "cookie": "^1.0.2",
    "date-fns": "^4.1.0",
    "date-fns-tz": "^3.2.0",
    "jsonwebtoken": "^9.0.2",
    "next": "^14.2.32",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.22",
    "dotenv-cli": "^7.4.4",
    "eslint": "^8.57.0",
    "eslint-config-next": "^14.2.32",
    "postcss": "^8.5.6",
    "prettier": "^3.2.0",
    "prisma": "^5.19.0",
    "tailwindcss": "^3.4.18"
  }
}
